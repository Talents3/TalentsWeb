<body>
	<div class="container" style="padding-left: 30%; padding-top: 5%; padding-right: 30%;">
   <!-- <div class="row text-center" style="display:flex; flex-wrap: wrap;"> -->

      <a href= 'http://talents3.com/users/<%= user.id %>'>
		
	    <div class="col-lg-3 col-sm-6"  >
	    	<div class="thumbnail">
	        <div class="card hovercard" style=" position: relative;
												  
												    overflow: hidden;
												    text-align: center;
												    background-color: rgba(214, 224, 226, 0.2);
												      padding-top: 20px;
												    margin: 10px 0 20px 0;
												  
												    border-top-width: 0;
												    border-bottom-width: 2px;
												    -webkit-border-radius: 3px;
												    -moz-border-radius: 3px;
												    border-radius: 3px;
												    -webkit-box-shadow: none;
												    -moz-box-shadow: none;
												    box-shadow: none;
												    -webkit-box-sizing: border-box;
												    -moz-box-sizing: border-box;
												    box-sizing: border-box;">
						<div class="cardheader" style="  background-image: url('http:talents3.com/api/v1/getBackground/mountain.jpg'); 
					    background-size: cover;
					    height: 100px; "> </div>
						<div class="avatar" style=" position: relative;
					    top: -50px;
					    margin-bottom: -50px; ">
												<% if(user.image!=null) { %>
							<img src='http://talents3.com/api/v1/getImages/<%= user.image%>' style= " width: 100px;
														    height: 100px;
														    max-width: 100px;
														    max-height: 100px;
														    -webkit-border-radius: 50%;
														    -moz-border-radius: 50%;
														    border-radius: 50%;
														    border: 5px solid rgba(255,255,255,0.5);" >
							<% } else { %>
							<img src="http://talents3.com/api/v1/getBackground/defaultImage.png" style="width: 100px;
														    height: 100px;
														    max-width: 100px;
														    max-height: 100px;
														    -webkit-border-radius: 50%;
														    -moz-border-radius: 50%;
														    border-radius: 50%;
														    border: 5px solid rgba(255,255,255,0.5);">
							<% } %>
						</div>

				  	<div class="info" style=" padding: 4px 8px 10px;">
							  <div class="title"><h5 class="card-title" style="color: red;"> <%= user.username %></h5></div>
							   <% if(user.description != null) { %>
							   <div class="desc" style="font-size: 14px;
								    color: #737373;
								    line-height: 2.5ex;
								    height: 5ex;
								    overflow: hidden;
								    text-overflow: ellipsis;"><%= user.description%></div>
							   <% } else { %>
							   <div class="desc" style=" font-size: 14px;
								    color: #737373;
								    line-height: 2.5ex;
								    height: 5ex;
								    overflow: hidden;
								    text-overflow: ellipsis;">Description</div>
								                                <% }  if (user.experiences.length > 0) { %>
		                       <div class="eduexp" style=" font-size: 14px;
							    color: #737373;
							    line-height: 2.5ex;
							    height: 2.5ex;
							    overflow: hidden;
							    text-overflow: ellipsis;"> <%= user.experiences[0].companyName %></div>
		                       <% } else { %>
                                <div class="eduexp" style="  font-size: 14px;
									    color: #737373;
									    line-height: 2.5ex;
									    height: 2.5ex;
									    overflow: hidden;
									    text-overflow: ellipsis;"> Experience</div>
                                <% }  if (user.educations.length > 0) { %>
                                <div class="eduexp" style=" font-size: 14px;
							    color: #737373;
							    line-height: 2.5ex;
							    height: 2.5ex;
							    overflow: hidden;
							    text-overflow: ellipsis;" > <%= user.educations[0].universityName %></div>
                                <% } else { %>
                                <div class="eduexp" style=" font-size: 14px;
									    color: #737373;
									    line-height: 2.5ex;
									    height: 2.5ex;
									    overflow: hidden;
									    text-overflow: ellipsis;">Education</div>
                                <%  } %>
								
				  	</div>
			    </div>
	        </div>
	      </div>
	  </a>
   </div>
</body>
